{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "trailingComma": "all",
  "overrides": [
    {
      "files": ".eslintrc",
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
